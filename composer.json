{
    "name" : "genkgo/push",
    "description": "Send push messages to Android and Apple using one interface.",
    "require" : {
        "php" : "^7.2",
        "ext-json" : "*",
        "php-gcm/php-gcm": "~1.1.1",
        "apple/apn-push": "^3.0",
        "guzzlehttp/guzzle": "^6.0",
        "lcobucci/jwt": "^3.3.1"
    },
    "require-dev" : {
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload" : {
        "psr-4" : {
            "Genkgo\\Push\\" : ["src"]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Genkgo\\Push\\" : ["test"]
        }
    }
}